  "scripts": {
    "mongo": "mongod --dbpath db",
    "bundle": "./node_modules/.bin/ts-node tools/run bundle",
    "build": "./node_modules/.bin/ts-node tools/run build",
    "dev": "./node_modules/.bin/ts-node tools/run runServer | nodemon --watch dist",
    "dev:watch": "./node_modules/.bin/ts-node tools/run runServer | nodemon --watch dist",
    "start": "./node_modules/.bin/ts-node tools/run start",
    "test": "mocha \"src/**/*.test.ts\" --require babel-register --require test/setup.js",
    "test:watch": "npm run test -- --reporter min --watch",
    "render": "./node_modules/.bin/ts-node tools/run render",
    "extractMessages": "./node_modules/.bin/ts-node tools/run extractMessages",
    "copy": "./node_modules/.bin/ts-node tools/run copy",
    "clean": "./node_modules/.bin/ts-node tools/run clean",
    "lint": "./node_modules/.bin/tslint --project tsconfig.json 'src/**/*.ts?(x)'"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/tools/jest.preprocessor.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts|tsx|js|jsx)$",
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/test/mocks/fileMock.js",
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "globals": {
      "DEVELOPMENT": false,
      "FAKE_SERVER": false
    }
